    "joi-to-typescript": "^1.14.1",
    "lint-staged": "^11.2.6",
    "minimist": "^1.2.8",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.8",
    "require-hijack": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.26.3",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "yorkie": "^2.0.0"
  }
