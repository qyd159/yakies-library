{
  "name": "@yakies/graph",
  "version": "0.3.0",
  "description": "图形渲染器",
  "main": "dist/index.js",
  "scripts": {
    "gen-types": "tsc -d --outDir dist --emitDeclarationOnly",
    "dev:esbuild": "npm run clean && concurrently \"postcss src/**/*.css --base src -d dist -w\" \"npm run gen-types && node scripts/build.js\"",
    "dev": "npm run clean && concurrently \"postcss src/**/*.css --base src -d dist -w\" \"rollup -c rollup.config.mjs --watch\"  \"tsc --outDir dist  --declaration --emitDeclarationOnly\"",
    "build": "postcss src/**/*.css --base src -d dist && rollup -c rollup.config.mjs",
    "clean": "rimraf dist/",
    "test": "npm-run-all --parallel test:*",
    "test:unit": "mocha",
    "test:integration": "karma start test/integration/karma.conf.js"
  },
  "author": "qinyadong",
  "license": "ISC",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@antv/hierarchy": "^0.6.7",
    "@hapi/joi": "^17.1.1",
    "@yakies/models": "^1.0.0",
