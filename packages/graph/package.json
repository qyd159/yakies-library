{
  "name": "@yakies/graph",
  "version": "0.3.1",
  "description": "图形渲染器",
  "main": "dist/index.js",
  "scripts": {
    "gen-types": "tsc -d --outDir dist --emitDeclarationOnly",
    "dev:esbuild": "npm run clean && concurrently \"postcss src/**/*.css --base src -d dist -w\" \"npm run gen-types && node scripts/build.js\"",
    "dev": "npm run clean && concurrently \"postcss src/**/*.css --base src -d dist -w\" \"rollup -c rollup.config.mjs --watch\"  \"tsc --outDir dist  --declaration --emitDeclarationOnly\"",
    "build": "postcss src/**/*.css --base src -d dist && rollup -c rollup.config.mjs",
    "clean": "rimraf dist/",
    "test": "npm-run-all --parallel test:*",
    "test:unit": "mocha",
    "test:integration": "karma start test/integration/karma.conf.js"
  },
  "author": "qinyadong",
  "license": "ISC",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@antv/hierarchy": "^0.6.11",
    "@hapi/joi": "^17.1.1",
    "@yakies/models": "^1.0.0",
    "acorn": "^8.10.0",
    "color": "^4.2.3",
    "d3": "^6.7.0",
    "debug": "^4.3.4",
    "fmin": "^0.0.2",
    "ignore-styles": "^5.0.1",
    "inpairs": "^1.0.3",
    "joi": "^17.9.2",
    "js-combinatorics": "^1.5.7",
    "jsdom": "^16.7.0",
    "kld-intersections": "^0.7.0",
    "lodash-es": "^4.17.21",
    "mix-with": "^3.2.4",
    "smooth-polyline": "^1.0.8",
    "treewalker": "^1.1.5",
    "tsccss": "^0.0.6",
    "venn.js": "^0.2.20",
    "xml-formatter": "^2.6.1",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/chai": "^4.3.5",
    "@types/color": "^3.0.3",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/hapi__joi": "^17.1.1",
    "@types/jquery": "^3.5.16",
    "@types/jsdom": "^16.2.15",
    "@types/lodash-es": "^4.17.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.54",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^9.8.8",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.7",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.15.18",
