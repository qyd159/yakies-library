    "acorn": "^8.5.0",
    "color": "^4.2.3",
    "d3": "^6.7.0",
    "debug": "^4.3.4",
    "fmin": "^0.0.2",
    "ignore-styles": "^5.0.1",
    "inpairs": "^1.0.3",
    "joi": "^17.4.0",
    "js-combinatorics": "^1.4.5",
    "jsdom": "^16.5.2",
    "kld-intersections": "^0.7.0",
    "lodash-es": "^4.17.21",
    "mix-with": "^3.1.2",
    "smooth-polyline": "^1.0.8",
    "treewalker": "^1.1.5",
    "tsccss": "^0.0.6",
    "venn.js": "^0.2.20",
    "xml-formatter": "^2.4.0",
    "yoga-layout-prebuilt": "^1.10.0"
  },
